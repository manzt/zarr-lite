{
  "name": "@manzt/zarr-lite",
  "version": "0.1.0",
  "description": "A partial Zarr Implementation for reading array chunks.",
  "repository": {
    "type": "git",
    "url": "https://github.com/manzt/zarr-lite.git"
  },
  "type": "module",
  "module": "index.js",
  "files": [
    "*.js",
    "*.d.ts"
  ],
  "exports": {
    ".": "./index.js",
    "./core": "./core.js",
    "./httpStore": "./httpStore.js"
  },
  "scripts": {
    "build": "rollup --format esm --dir dist src/index.js src/core.js src/httpStore.js",
    "postbuild": "cp -r types/* dist"
  },
  "keywords": [
    "zarr",
    "ndarray",
    "async"
  ],
  "author": "Trevor Manz",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "rollup": "^2.34.1",
    "zarrita": "^0.1.3"
  }
}
